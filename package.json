{
  "name": "hooks",
  "private": true,
  "description": "Custom react hooks.",
  "homepage": "https://js.work",
  "keywords": [
    "typescript",
    "react",
    "npm",
    "lerna",
    "hooks",
    "react-hooks"
  ],
  "license": "MIT",
  "workspaces": [
    "./lib"
  ],
  "scripts": {
    "clean": "lerna clean -y",
    "format": "run-p format:*",
    "format:code": "npm run lint:code -- --fix",
    "format:other": "npm run lint:other -- --write",
    "test": "jest",
    "test:watch": "jest --watch --silent",
    "build": "lerna run build --stream",
    "build:lib": "lerna run --scope hooks build && npm run copy:hooks",
    "dev:lib": "npm run test:watch",
    "publish": "npm run build:lib && npm run test && lerna publish --no-git-reset"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.38",
    "@types/react-dom": ">=16.8.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "all-contributors-cli": "^6.20.0",
    "chalk": "^4.1.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "jest": "^27.5.1",
    "jest-preset-gatsby": "^1.0.134",
    "jsdom": "^19.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.3",
    "npm-run-all": "^4.1.5",
    "plop": "^3.0.5",
    "prettier": "^2.5.1",
    "react-test-renderer": "^18.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
