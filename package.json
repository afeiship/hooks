{
  "name": "hooks",
  "private": true,
  "description": "Custom react hooks.",
  "homepage": "https://js.work",
  "keywords": [
    "typescript",
    "react",
    "npm",
    "lerna",
    "hooks",
    "react-hooks"
  ],
  "license": "MIT",
  "workspaces": [
    "./lib"
  ],
  "scripts": {
    "clean": "lerna clean -y",
    "format": "run-p format:*",
    "format:code": "npm run lint:code -- --fix",
    "format:other": "npm run lint:other -- --write",
    "test": "jest",
    "test:watch": "jest --watch --silent",
    "build": "lerna run build --stream",
    "build:lib": "lerna run --scope hooks build && npm run copy:hooks",
    "dev:lib": "npm run test:watch",
    "publish": "npm run build:lib && npm run test && lerna publish --no-git-reset"
  },
  "devDependencies": {
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "8.0.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": ">=18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "chalk": "^5.0.1",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-preset-gatsby": "^1.0.134",
    "jsdom": "^19.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  }
}
